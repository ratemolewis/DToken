    "buffer": "6.0.3",
    "copy-webpack-plugin": "^9.0.1",
    "events": "3.3.0",
    "html-webpack-plugin": "5.5.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "terser-webpack-plugin": "5.2.5",
    "util": "0.12.4",
    "webpack": "5.63.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.4.0"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
